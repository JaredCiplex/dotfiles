# freshshell.com
#
# Examples:
#
#   fresh twe4ked/dotfiles shell/functions.sh
#   fresh jasoncodes/dotfiles aliases/rails.sh
#   fresh jasoncodes/dotfiles config/tmux.conf --file
#
# See http://freshshell.com/readme for documentation.

fresh freshshell/fresh bin/fresh --bin

# .zshrc build
fresh-options --file=~/.zshrc --marker
  fresh shell/zsh/zshrc
fresh-options

# .zlogin build
fresh-options --file=~/.zlogin --marker
  fresh shell/zsh/zlogin
fresh-options

# oh-my-zsh and my own custom
fresh robbyrussell/oh-my-zsh . --file=~/.oh-my-zsh/